{
  "name": "ico-tend",
  "version": "1.0.0",
  "description": "The TEND ICO.",
  "author": "Validity Labs AG <info@validitylabs.org>",
  "homepage": "https://github.com/validitylabs/ico-tend",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "^8.x"
  },
  "scripts": {
    "start": "yarn develop",
    "coverage": "shx rm -fr build; solidity-coverage",
    "compile": "truffle compile --all",
    "dev": "truffle develop",
    "migrate-ropsten": "shx rm -fr build; cross-env NODE_ENV=ropsten truffle migrate --reset --compile-all --network ropsten",
    "clean": "shx rm -fr dist build bin coverage coverage.json",
    "reset": "npm run clean; shx rm -fr node_modules",
    "upgrade": "yarn run reset; yarn upgrade",
    "postinstall": "node ./tools/hashChecker.js",
    "test": "truffle test --network _development",
    "migrate": "yarn migrate-dev",
    "migrate-dev": "truffle migrate --reset --compile-all --network _development",
    "testrpc": "truffle develop --log --gasLimit 0x44aa20 --mnemonic 'waste system voyage dentist fine donate purpose truly cactus chest coyote globe'"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "moment": "^2.19.3",
    "truffle-contract": "^3.0.1",
    "web3": "^1.0.0-beta.26",
    "winston": "^2.4.0",
    "zeppelin-solidity": "^1.6.0"
  },
  "devDependencies": {
    "@validitylabs/js-utils": "^0.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "ganache-core": "^2.0.2",
    "jsdoc": "^3.5.5",
    "keythereum": "^1.0.2",
    "shelljs": "^0.7.8",
    "shx": "^0.2.2",
    "solc": "0.4.18",
    "solidity-coverage": "git://github.com/validitylabs/solidity-coverage.git",
    "superagent": "^3.8.1",
    "tree-kill": "^1.2.0",
    "truffle": "^4.0.1"
  },
  "optionalDevDependencies": {
    "fsevents": "*"
  }
}
